<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.troublefixer.mapper.HistoryMapper">
    <select id="findPathById" parameterType="Integer" resultType="String">
        select model_path from history where model_id = #{modelId};
